<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="94.0" prefWidth="316.0" stylesheets="@../utilerias/general/Estilo.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="vistas.ConfirmacionExportacionController">
   <ImageView fitHeight="40.0" fitWidth="21.0" layoutX="26.0" layoutY="22.0" pickOnBounds="true" preserveRatio="true">
      <image>
         <Image url="@../../../../Downloads/icon-advertencia.png" />
      </image>
   </ImageView>
   <Label layoutX="55.0" layoutY="22.0" text="¿Está seguro de exportar el respaldo?" textAlignment="CENTER">
      <font>
         <Font name="Montserrat Medium" size="12.0" />
      </font>
   </Label>
   <HBox layoutX="20.0" layoutY="56.0" prefHeight="47.0" prefWidth="268.0" spacing="89.0">
      <children>
         <Button onAction="#confirmarExportacion" prefHeight="35.0" prefWidth="90.0" styleClass="button-gris" text="Sí" textAlignment="CENTER">
            <opaqueInsets>
               <Insets />
            </opaqueInsets>
         </Button>
         <Button onAction="#cancelarExportacion" prefHeight="35.0" prefWidth="90.0" styleClass="button-gris" text="Cancelar" textAlignment="CENTER">
            <opaqueInsets>
               <Insets />
            </opaqueInsets>
         </Button>
      </children>
   </HBox>
</AnchorPane>